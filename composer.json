{
  "name": "ksjogo/typo3-docker",
  "description": "TYPO3 dev docker",
  "license": "GPL-3.0",
  "authors": [
 ],
  "config": {
    "bin-dir": "bin",
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "replace": {
    "typo3-cms-extension/extbase": "*",
    "typo3-cms-extension/fluid": "*",
    "typo3-cms-extension/version": "*"
  },
  "require": {
    "typo3/cms": "dev-master",
    "typo3-ter/devlog": "*",
    "helhum/typo3-console": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.3",
    "squizlabs/php_codesniffer": "^2.6"
  },
  "extra": {
    "typo3/cms": {
    },
    "helhum/typo3-console": {
      "install-binary": false,
      "active-packages": [
                ]
    }
  }
}
